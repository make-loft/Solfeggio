<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:system="clr-namespace:System;assembly=mscorlib">

	<system:Double
		x:Key="MagnitideSpectrumStrokeThickness">1</system:Double>

	<system:Double
		x:Key="WaveInStrokeThickness">1</system:Double>

	<system:Double
		x:Key="WaveOutStrokeThickness">1</system:Double>

	<system:Double
		x:Key="MinDominantOpacityLevel">0.3</system:Double>

	<system:Double
		x:Key="InfoTextOpacityLevel">0.4</system:Double>

	<system:String
		x:Key="DefaultStringFormat">F2</system:String>

</ResourceDictionary>