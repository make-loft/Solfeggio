<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:system="clr-namespace:System;assembly=mscorlib">

	<system:Double
		x:Key="PlusSinglePi">+3.1415926535897931</system:Double>
	
	<system:Double
		x:Key="MinusSinglePi">-3.1415926535897931</system:Double>

	<system:Double
		x:Key="PlusSingleE">+2.7182818284590451</system:Double>

	<system:Double
		x:Key="MinusSingleE">-2.7182818284590451</system:Double>

	<system:Double
		x:Key="MagnitideSpectrumStrokeThickness">1</system:Double>

	<system:Double
		x:Key="WaveInStrokeThickness">1</system:Double>

	<system:Double
		x:Key="WaveOutStrokeThickness">1</system:Double>

	<system:Double
		x:Key="MinHarmonicOpacityLevel">0.3</system:Double>

	<system:Double
		x:Key="MinWidth">0</system:Double>

	<system:Double
		x:Key="InfoTextOpacityLevel">0.4</system:Double>

	<system:String
		x:Key="DefaultStringFormat">F2</system:String>

	<Thickness
		x:Key="RegularMargin">4</Thickness>

	<Visibility
		x:Key="MagnitudeSpectrumVisibility.FFT">Collapsed</Visibility>
	<Visibility
		x:Key="MagnitudeSpectrumVisibility.PMI">Visible</Visibility>

	<Visibility
		x:Key="PhaseSpectrumVisibility.FFT">Collapsed</Visibility>
	<Visibility
		x:Key="PhaseSpectrumVisibility.PMI">Collapsed</Visibility>

	<Visibility
		x:Key="FrameVisibility.Rectangle">Collapsed</Visibility>
	<Visibility
		x:Key="FrameVisibility.Window">Collapsed</Visibility>

</ResourceDictionary>