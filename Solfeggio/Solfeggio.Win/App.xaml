<Application 
	x:Class="Solfeggio.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:converters="clr-namespace:Solfeggio.Converters"
	xmlns:m="clr-namespace:Ace.Converters;assembly=Ace.Zest"
	StartupUri="AppView.xaml"
	Startup="App_OnStartup"
	Exit="App_OnExit">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary
					Source="pack://application:,,,/Ace;component/Ace.Zest/Dictionaries/AppConverters.xaml" />
				<ResourceDictionary
					Source="Dictionaries/Templates.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
